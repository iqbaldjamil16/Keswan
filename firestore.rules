
rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // For this simple application, allow anyone to read and write to the
    // healthcareServices collection. In a real-world application, you would
    // want to lock this down with more specific rules.
    match /healthcareServices/{serviceId} {
      allow read, write: if true;
    }
  }
}
