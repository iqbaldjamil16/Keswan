rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // The application does not implement user-specific data access.
    // To allow the app to function as designed, this collection is public.
    // For a production app with real user data, you should implement
    // authentication and user-based security rules.
    match /healthcareServices/{serviceId} {
      allow read, write: if true;
    }
  }
}
